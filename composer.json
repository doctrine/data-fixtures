{
    "name": "doctrine/data-fixtures",
    "type": "library",
    "description": "Data Fixtures for all Doctrine tools",
    "keywords": ["database", "fixture"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {"name": "Jonathan Wage", "email": "jonwage@gmail.com"},
        {"name": "Guilherme Blanco", "email": "guilhermeblanco@hotmail.com"}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "doctrine/dbal": "~2.4",
        "doctrine/orm": "~2.4"
    },
    "suggest": {
        "doctrine/dbal": "For loading DBAL fixtures",
        "doctrine/orm": "For loading ORM fixtures",
        "doctrine/mongodb": "For loading MongoDB fixtures",
        "doctrine/mongodb-odm": "For loading MongoDB ODM fixtures",
        "doctrine/phpcr-odm": "For loading PHPCR ODM fixtures"
    },
    "autoload": {
        "psr-0": { "Doctrine\\Fixture": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
